{
  "name": "dev-utilities",
  "displayName": "Dev Utilities",
  "description": "A versatile VS Code extension with essential utilities for various development tasks",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "AshishPawar",
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "images/DevUtils_Transparent.png",
  "contributes": {
    "commands": [
      {
        "command": "dev-utils.open",
        "title": "Open Dev Utilities"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
